{
  "name": "@feichtmedia/imagehandler-react-sdk",
  "version": "1.6.4",
  "description": "React SDK for FeichtMedia ImageManager and ImageHandler for use inside a React application.",
  "author": {
    "name": "FeichtMedia",
    "url": "https://www.feicht-media.de/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/feichtmedia/imagehandler-react-component"
  },
  "bugs": {
    "url": "https://github.com/feichtmedia/imagehandler-react-component/issues"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "tsc -w",
    "build": "rm -r ./dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
